<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.cnbs.recyclerviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".indexRV.IndexRVActivity"/>
        <activity android:name=".mulitRV.MulitActivity"/>
        <activity android:name=".refreshloadRV.RVActivity"/>
        <activity android:name=".refreshloadRV.SuperRVActivity"/>
        <activity android:name=".vlayoutRV.EShopActivity"/>
        <activity android:name=".groupRV.GroupActivity">
        </activity>
    </application>

</manifest>